{
  "name": "markgit",
  "version": "4.0.0",
  "description": "Use Github Repo as a Database to Store Files. Retrieve, Create, Update, and Delete Markdown, JSON, and Image Files and Folders. Get the List of Files From a Repo",
  "main": "./dist/markgit-node.js",
  "module": "./dist/markgit-node.esm.js",
  "browser": "./dist/markgit.js",
  "exports": {
    ".": {
      "import": "./dist/markgit-node.esm.js",
      "require": "./dist/markgit-node.js",
      "browser": "./dist/markgit.esm.js"
    }
  },
  "scripts": {
    "build:js": "tsc --project tsconfig.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:bundle": "webpack",
    "build": "npm run build:bundle"
  },
  "keywords": [
    "github",
    "markdown",
    "json",
    "images",
    "github database",
    "markdown to html",
    "github api",
    "file management",
    "create files",
    "update files",
    "delete files",
    "image files",
    "json files"
  ],
  "author": "Sidmaz666",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "highlight.js": "^11.7.0",
    "markdown-it": "^13.0.1",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sidmaz666/markgit.git"
  },
  "bugs": {
    "url": "https://github.com/Sidmaz666/markgit/issues"
  },
  "homepage": "https://github.com/Sidmaz666/markgit#readme"
}
